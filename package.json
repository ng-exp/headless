{
  "name": "headless-project",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build @ng-exp/headless-ui --prod",
    "test": "ng test @ng-exp/headless-ui --browsers=ChromeHeadless --watch=false",
    "lint": "ng lint @ng-exp/headless-ui",
    "e2e": "ng e2e"
